{
  "name": "somatik",
  "version": "19.0.0",
  "private": true,
  "description": "Somatik Web Portal",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.11.2",
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.7.x",
    "branch-name": "1.0.0",
    "browser-sync": "2.26.7",
    "css-mqpacker": "7.0.0",
    "del": "5.1.0",
    "fancy-log": "1.3.3",
    "gh-pages": "2.1.1",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.2.0",
    "gulp-image-resize": "0.13.1",
    "gulp-imagemin": "6.1.1",
    "gulp-pipeline": "4.0.43",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svgo": "2.1.1",
    "gulp-svgstore": "7.0.1",
    "pug": "2.0.4",
    "stylelint": "11.1.1",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
