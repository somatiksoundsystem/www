{
  "name": "somatik",
  "version": "19.0.0",
  "private": true,
  "description": "Somatik Web Portal",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.1.x",
    "browser-sync": "^2.26.5",
    "css-mqpacker": "7.0.0",
    "del": "4.1.1",
    "gh-pages": "2.0.1",
    "gulp": "^4.0.1",
    "gulp-clean-css": "4.2.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-pipeline": "^4.0.43",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.5",
    "stylelint": "^9.10.1",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp start",
    "deploy": "gulp deploy"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "10"
  }
}
